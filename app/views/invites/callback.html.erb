<% if @contacts != nil %>
	<a>List of <%= @user.name %>'s contacts from Gmail</a>
	<ul>
	<% @contacts.each do |contact| %>
	  <li><%= contact.name contact.email %>
	<% end %>
	</ul>
<% else %>
	<table>
	<th>key</th>
	<th>Value</th>
	<% for item in request.env %>
	<tr>
	<td><%= item[0] %></td>
	<td><%= item[1] %></td>
	</tr>
	<% end %>
	</table>
<% end %>